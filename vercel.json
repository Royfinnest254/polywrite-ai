{
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "frontend/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/app/(.*)",
            "dest": "/frontend/$1"
        },
        {
            "src": "/app",
            "dest": "/frontend/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ]
}